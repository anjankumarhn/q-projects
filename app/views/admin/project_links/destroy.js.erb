<% if @success %>

  	// Fill the right box with first project_link details
  	$('#div_project_links').html("<%= escape_javascript(render(:partial=>'index', :locals=>{:project => @project})) %>");

<% else %>

  var noResultsText = "<div id=\"div_admin_empty\" class=\"box text-color-grey p-80 text-align-center\" style=\"height:200px;\"><%= translate("forms.no_results") %></div>";
  $('#div_project_links').html(noResultsText);

<% end %>
